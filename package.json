{
  "name": "sistema-empresarial",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "server": "node src/server.js",
    "start": "npm run server",
    "build:prod": "npm run env:gcp && npm run build",
    "env:dev": "node config-manager.js switch dev",
    "env:prod": "node config-manager.js switch prod",
    "env:gcp": "node config-manager.js switch gcp",
    "env:test": "node config-manager.js switch test",
    "env:show": "node config-manager.js show",
    "env:list": "node config-manager.js list",
    "dev:full": "npm run env:dev && npm run server & npm run dev",
    "deploy:gcp": "chmod +x ./scripts/deploy-gcp.sh && ./scripts/deploy-gcp.sh",
    "gcp:build": "gcloud builds submit --tag gcr.io/pros-sistema-empresarial/pros-app .",
    "gcp:deploy": "gcloud run deploy pros-app --image gcr.io/pros-sistema-empresarial/pros-app --region us-central1 --platform managed --allow-unauthenticated",
    "db:init": "node scripts/init-cloud-database.js",
    "db:reset": "node scripts/init-cloud-database.js",
    "db:tables": "node scripts/create-tables-only.js"
  },
